#include "CommWebImageObject.h"CommWebImageObject::CommWebImageObject (void){	MapTools [0] = G42SingleImageViewer::MapEditTool;	MapTools [1] = G42SingleImageViewer::MapCircleTool;	MapTools [2] = G42SingleImageViewer::MapPointTool;	MapTools [3] = G42SingleImageViewer::MapPolygonTool;	MapTools [4] = G42SingleImageViewer::MapRectangleTool;}CommWebImageObject::~CommWebImageObject(){ }/*  ADD things that are not in webimage.h but for mac need to be  */voidCommWebImageObject::SetMapTool (int menuIndex){	G42SingleImageViewer::G42MapTool tool = MapTools [menuIndex];	if (View)		View -> SetMapTool (tool);}voidCommWebImageObject::DrawImageMapElement (uint index){	#if 0	// Library changed to not drawing map elements separately	if (View)		View -> DrawImageMapElement (index);	#endif}doubleCommWebImageObject::GetZoom (void){	double returnVal = 1.0;	if (View)		returnVal = View -> GetZoom();	return returnVal;}voidCommWebImageObject::SetImageChanged (Boolean changed){	if (Image)		Image -> SetDirty (changed);}